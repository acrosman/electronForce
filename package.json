{
  "name": "electron-force",
  "version": "0.0.1",
  "description": "An Electron Wrapper on JSForce",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint src --ignore-path .gitignore",
    "test-on-commit": "jest --passWithNoTests",
    "prepare": "husky install",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "make": "electron-forge make",
    "test": "jest --coverage --passWithNoTests"
  },
  "keywords": [
    "Electron",
    "JSForce",
    "Salesforce"
  ],
  "author": "Aaron Crosman <acrosman@gmail.com> (https://spinningcode.org)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/acrosman/electronForce"
  },
  "dependencies": {
    "bootstrap": "^5",
    "bootswatch": "^5",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.1",
    "jquery.json-viewer": "^1.5.0",
    "jsforce": "^3",
    "minimist": "^1.2.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7",
    "@electron-forge/maker-deb": "^7",
    "@electron-forge/maker-dmg": "^7",
    "@electron-forge/maker-rpm": "^7",
    "@electron-forge/maker-squirrel": "^7",
    "@electron-forge/maker-zip": "^7",
    "@electron-forge/publisher-github": "^7",
    "debug-menu": "^0.6.1",
    "electron": "^36",
    "electron-debug": "^2.2.0",
    "electron-reloader": "^1.1.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0",
    "eslint-config-airbnb-base": "^15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0"
  }
}
